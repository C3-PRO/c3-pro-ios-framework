{
  "id": "example-question-dates",
  "resourceType": "Questionnaire",
  "status": "draft",
  "title": "This survey contains samples to show how date and time FHIR Questionnaires translate to ResearchKit surveys",
  "date": "2016-04-11",
  "item": [
    {
      "type": "group",
      "fhir_comments": "Note how groups without text will not show an individual instruction step",
      "item": [
        {
          "linkId": "dateDate",
          "text": "When were you born?",
          "type": "date"
        },
        {
          "linkId": "dateDateTime",
          "text": "Choose a date and a time",
          "type": "dateTime"
        },
        {
          "linkId": "dateTimeOfDay",
          "text": "What time did you wake up today?",
          "type": "time"
        }
      ]
    },
    {
      "text": "Final Step",
      "type": "display",
      "extension": [
        {
          "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-instruction",
          "valueString": "Pressing “Done” will complete this survey."
        }
      ]
    }
  ]
}
